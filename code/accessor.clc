
class Accessor {
    private int field1;
    private int field2;

    Accessor(int a, int b) {
        self.field1 = a;
        self.field2 = b;
    }

    void set_field1(int a) {
        self.field1 = a;
    }

    void set_field2(int b) {
        self.field2 = b;
    }

    int get_field1() {
        return self.field1;
    }

    int get_field2() {
        return self.field2;
    }
}
