	$(INSTALL) -m 644 GenericsParam7.clo "$(datarootdir)"
	rm -f "$(DESTDIR)/$(bindir)/clover"
	if echo $(CFLAGS) | grep -q MDEBUG; then $(INSTALL) -m 755 clover "$(DESTDIR)/$(bindir)"; else $(INSTALL) -s -m 755 clover "$(DESTDIR)/$(bindir)"; fi;
